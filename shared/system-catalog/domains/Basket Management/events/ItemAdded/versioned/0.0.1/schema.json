{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "metadata":{
      "type": "object",
      "properties": {
        "basketId": {
          "type": "string"
        },
        "productId": {
          "type": "string"
        },
        "userId": {
          "type": "string"
        },
        "submitTime": {
          "type": "string",
          "format": "date-time",
          "description": "The DateTime with offset or UTC date time"
        }
      }
    },
    "data": {
      "type": "object",
      "properties": {
        "itemCount":{
          "type": "integer",
          "minimum": 1
        }
      }
    }
  },
  "required": [
    "metadata",
    "data"
  ]
}